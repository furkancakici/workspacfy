{
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 180,
    "tabWidth": 4,
    "useTabs": false,
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxSingleQuote": false,
    "endOfLine": "lf",
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": [
        "^(dotenv)(.*)$",
        "<THIRD_PARTY_MODULES>",
        "^@/config/(.*)$",
        "^@/models/(.*)$",
        "^@/enums/(.*)$",
        "^@/services/(.*)$",
        "^@/controllers/(.*)$",
        "^@/middlewares/(.*)$",
        "^@/routes/(.*)$",
        "^@/validation/(.*)$",
        "^@/utils/(.*)$",
        "^@/seeders/(.*)$",
        "^@/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderGroupNamespaceSpecifiers": true,
    "importOrderCaseInsensitive": true
}
